<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="150.0" prefWidth="225.0" spacing="5.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gardenplanner.gardenplanner.controller.GardenMemberController">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="20.0" prefWidth="140.0" text="Members" textAlignment="CENTER" />
      <ListView fx:id="memberList" prefHeight="80.0">
         <VBox.margin>
            <Insets left="40.0" right="40.0" />
         </VBox.margin>
      </ListView>
      <Button fx:id="confirmButton" alignment="CENTER" mnemonicParsing="false" onAction="#onConfirmButton" prefHeight="30.0" prefWidth="80.0" text="Confirm" />
   </children>
</VBox>
